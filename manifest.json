{
  "manifest_version": 3,
  "name": "AI 2 AI",
  "version": "1.0.0",
  "description": "Replaces mentions of 'AI' on LinkedIn with Allen Iverson fun facts",
  "host_permissions": [
    "https://*.linkedin.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.linkedin.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["ai2ai_backgroundv3.jpg", "Basketball.otf", "Icons/v2Newai2ai_icon_16x16.png","Icons/v2Newai2ai_icon_32x32.png",
        "Icons/v2Newai2ai_icon_48x48.png"],
      "matches": ["https://*.linkedin.com/*"]
    }
  ],
  "icons": {
    "16": "Icons/v2Newai2ai_icon_16x16.png",
    "32": "Icons/v2Newai2ai_icon_32x32.png",
    "48": "Icons/v2Newai2ai_icon_48x48.png"
  },
  "action": {
    "default_icon": {
      "16": "Icons/v2Newai2ai_icon_16x16.png",
      "32": "Icons/v2Newai2ai_icon_32x32.png",
      "48": "Icons/v2Newai2ai_icon_48x48.png"
    }
  }
}
